   "source": [
    "def calculate_mean(nums):\n",
    "    ans = 0\n",
    "    for i in nums:\n",
    "        ans += i\n",
    "    ans /= len(nums)\n",
    "\n",
    "    return ans\n",
    "\n",
    "data = [10, 15, 20, 25, 30]\n",
    "mean_value = calculate_mean(data)\n",
    "print(\"Mean:\", mean_value)\n"
   ]
  }
